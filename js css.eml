body {
    font-family: Arial, sans-serif;
    background-color: #f5f5f5;
    margin: 0;
    padding: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    min-height: 100vh;
    animation: colorChange 10s infinite alternate;
    /* Add animation properties */

}

@keyframes colorChange {
    0% {
        background-color: #f5f5f5;
        /* Initial background color */
    }

    100% {
        background-color: #FF5733;
        /* Target background color */
    }
}

h1 {
    text-align: center;
}

.container {
    display: flex;
    width: 90%;
    margin: auto;
}

.form-container {
    background-color: #eee4e4;
    border: 1px solid #ccc;
    padding: 20px;
    border-radius: 5px;
    width: 30%;
}

.details-container {
    text-align: center;
    background-color: #eee4e4;
    border: 1px solid #ccc;
    padding: 20px;
    border-radius: 5px;
    width: 100%;
}

.form-container {
    margin-right: 10px;
}

.user-image {
    max-width: 100px;
    height: 100px;
    border-radius: 10px;
}

form {
    display: flex;
    flex-direction: column;
}

label {
    margin: 5px 0;
}

input {
    padding: 10px;
    margin: 5px 0;
}

select,
button {
    padding: 10px;
    margin: 5px 0;
}

button {
    background-color: #007BFF;
    color: #fff;
    border: none;
    cursor: pointer;
}

table {
    border-collapse: collapse;
    width: 100%;
}

table,
th,
td {
    border: 2px solid #000000;
}

th,
td {
    padding: 8px;
    text-align: left;
}

@media (max-width: 768px) {
    .container {
        flex-direction: column;
    }

    .form-container,
    .details-container {
        width: 100%;
        margin-right: 0;
    }
}

